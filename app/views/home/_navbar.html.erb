<div class="topnav">
  <%= link_to root_path, class: "active navbar-brand" do %>
    AvaDevHub <%= image_tag("ava-logo.png", alt: "Ava Logo") %>
  <% end %>
    <%= link_to "Meetings", meetings_path %>
    <%= link_to "Discussion Items", lists_path %>
  <div class="right-nav">
    <% if user_signed_in? %>
      <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "anti-button" %>
