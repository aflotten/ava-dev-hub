<div class="container text-center">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Title:</strong>
    <%= @list.title %>
  </p>

  <p>
    <strong>Notes:</strong>
    <p>
      <%= @list.notes %>
    </p>
  </p>

  <p>
    <strong>Completed:</strong>
    <%= @list.completed %>
  </p>

  <%= link_to 'Edit', edit_list_path(@list), class: "custom-link" %> |
  <%= link_to 'Back', lists_path, class: "custom-link" %>
</div>
